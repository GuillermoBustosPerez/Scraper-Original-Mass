@article{kuhn_unpacking_1991,
	title = {“{Unpacking}” {Reduction}: {Lithic} {Raw} {Material} {Economy} in the {Mousterian} of {West}-{Central} {Italy}},
	volume = {10},
	issn = {0278-4165/91},
	shorttitle = {“{Unpacking}” {Reduction}: {Lithic} {Raw} {Material} {Economy} in the {Mousterian} of {West}-{Central} {Italy}},
	abstract = {The extent to which lithic artifacts are consumed or reduced can be attributed to a number of factors, including raw material accessibility, differential transport, patterns of site use, and tool function. In order to isolate the influence of any single factor, independent data must be used to control for variation caused by the other factors. Variation in the reduction of cores and retouched tools in eight Mousterian assemblages from west-central Italy reflects the effects of several processes and contingencies. The availability of lithic raw materials strongly affects the extent of core exploitation but not the intensity of tool retouch or reduction. Evidence for differential transport accounts for some but not all of the remaining contrasts in tool reduction. Information derived from associated faunas suggests that contrasts in the duration or stability of cave-use events as well as activity variation stand behind some of the most pronounced differences in the intensity of reduction among the lithic assemblages.},
	journal = {Journal of Anthropological Archaeology},
	author = {Kuhn, Steven L.},
	year = {1991},
	keywords = {Middle Paleolithic, Mousterian, Lithic provisioning strategies},
	pages = {76--106},
}

@article{kuhn_planning_1992,
	title = {On {Planning} and {Curated} {Technologies} in the {Middle} {Paleolithic}},
	volume = {48},
	shorttitle = {On {Planning} and {Curated} {Technologies} in the {Middle} {Paleolithic}},
	abstract = {The importance of planning or anticipatory organization in the technologies of archaic hominids has recently come under much scrutiny. Among modern populations, the extent to which technologies are planned depends on a varnty of factors external to and internal to the cultural system, including the distribution of raw materials and settlement mobility. While data on the European Mousterian suggest that the manufacture and maintenance of tools were planned to a limited degree, evidence of truly long-term projection of needs is sparse. Most of the extant information is insufficient to evaluate whether these facts directly implicate the capacities of Mousterian hominids or whether they reflect the operation of independent factors that rendered more complex forms of planning unnecessary or impractical. Studies of a serns of Italian Paleolithic sites employing independent data to control for the contexts of stone tool manufacture and use suggest that apparent differences in technological planning among Middle and late Upper Paleolithic populations in that region may be explicable in terms of contrasting patterns of land use.},
	number = {3},
	journal = {Journal of Anthropological Research},
	author = {Kuhn, Steven L.},
	year = {1992},
	keywords = {Middle Paleolithic, mobility patterns, curated tools, Toolkit},
	pages = {185--214},
}

@article{morales_measuring_2015,
	title = {Measuring {Retouch} {Intensity} in {Lithic} {Tools}: {A} {New} {Proposal} {Using} {3D} {Scan} {Data}},
	volume = {22},
	shorttitle = {Measuring {Retouch} {Intensity} in {Lithic} {Tools}: {A} {New} {Proposal} {Using} {3D} {Scan} {Data}},
	doi = {10.1007/s10816-013-9189-0},
	abstract = {Identifying reduction dynamics in prehistoric retouched stone tools is important for understanding technological trends, as well as site function, raw material management and mobility of nomadic hunter-gatherers. In the absence of refits, the final state of the abandoned piece is the only remain that archaeologists have for the study of lithic reduction. The establishment of experimentally tested indexes providing strong correlations between estimated and real reduction are needed. In our work, we propose a new procedure for estimate reduction percentage. In this proposal, data are obtained from high resolution three-dimensional (3D) scans and manual measurements are avoided. The experimental test has been realized using distally retouched tools, the less suitable tools for reduction studies. The correlation levels obtained between the measured data and the estimated data increase substantially the original published from manual measurement, reaching a r2 value of 0.81 and a correlation of 0.9. The main contribution of the proposed method is the very high correlation obtained in the volume estimation (r2=0.89 and r=0.94) using the 3D-based measurements. The 3D models of the unretouched and retouched flakes used in this work are free to use and accessible through an online repository.},
	number = {2},
	journal = {Journal of Archaeological Method and Theory},
	author = {Morales, Juan Ignacio and Lorenzo, Carlos and Vergès, Josep María},
	year = {2015},
	keywords = {Lithic technology, 3D scanning, Reduction index, Volume estimation, Endscrapers},
	pages = {543--558},
	file = {Morales et al. - 2015 - Measuring Retouch Intensity in Lithic Tools A New.pdf:C\:\\Users\\guillermo_bustos_perez\\Zotero\\storage\\KWW8CTLP\\Morales et al. - 2015 - Measuring Retouch Intensity in Lithic Tools A New.pdf:application/pdf},
}

@article{bustos-perez_exploring_2019,
	title = {Exploring volume lost in retouched artifacts using height of retouch and length of retouched edge},
	volume = {27},
	issn = {2352409X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S2352409X19300045},
	doi = {10.1016/j.jasrep.2019.101922},
	abstract = {Retouched artifacts are one of the most common elements of Middle Paleolithic lithic assemblages. Resharpening of the edges by retouch is integral to key concepts of lithic technology and patterns of site occupation dynamics. This has led to the development of indexes that quantify retouch among artifacts, allowing comparisons according to raw material type, distance to raw material source, archaeological levels, and other factors. Several methods have been proposed for measuring the edge consumed by retouch on each artifact. These indexes are usually scaled, ranging from zero to one or from zero to 100. However, few indexes measure retouch on a nonlimited scale. Here we present a new index that combines height of retouch and length of retouched edge to obtain proxies of volume lost. Advantages and limitations of the index are experimentally explored through three patterns of retouch and then applied to two different levels of Esquilleu Cave. Results show how a two-pronged index that considers consumed edge and total of volume lost by retouch can deliver insights into resharpening strategies among Middle Paleolithic groups.},
	language = {en},
	urldate = {2019-10-01},
	journal = {Journal of Archaeological Science: Reports},
	author = {Bustos-Pérez, Guillermo and Baena, Javier},
	month = oct,
	year = {2019},
	pages = {101922},
	file = {Bustos-Pérez y Baena - 2019 - Exploring volume lost in retouched artifacts using.pdf:C\:\\Users\\guillermo_bustos_perez\\Zotero\\storage\\8TN9VCLJ\\Bustos-Pérez y Baena - 2019 - Exploring volume lost in retouched artifacts using.pdf:application/pdf},
}

@article{kuhn_geometric_1990,
	title = {A {Geometric} {Index} of {Reduction} for {Unifacial} {Stone} {Tools}},
	volume = {17},
	issn = {030543/90/050583 + 11\$ 03.00/0},
	abstract = {Issues relating to the maintenance and renewal of stone tools have become increasingly important in studies of prehistoric technologies. This paper presents an index of reduction for unifacial side-scrapers, a common tool form in many assemblages. The index is based on a simple geometric model of flake cross-sections. It is unprejudiced by varying paths of reduction and the original sizes of tools. However, the index is potentially subject to bias resulting from variation in the form of tooI blanks: in spite of these constraints, experimental data suggest that the index constitutes’s reliable measure of the extent of tool reduction.},
	journal = {Journal of Archaeological Science},
	author = {Kuhn, Steven L.},
	year = {1990},
	keywords = {Lithic technology, experimental archaeology, reduction index, GIUR, tool reduction},
	pages = {583--593},
	file = {Kuhn - 1990 - A Geometric Index of Reduction for Unifacial Stone.pdf:C\:\\Users\\guillermo_bustos_perez\\Zotero\\storage\\YY333FC7\\Kuhn - 1990 - A Geometric Index of Reduction for Unifacial Stone.pdf:application/pdf},
}

@article{eren_kuhns_2009,
	title = {Kuhn’s {Geometric} {Index} of {Unifacial} {Stone} {Tool} {Reduction} ({GIUR}): does it measure missing flake mass?},
	volume = {36},
	doi = {10.1016/j.jas.2009.01.011},
	abstract = {Kuhn’s GIUR is a set of measurements taken on a resharpened stone flake’s edge. Its purpose is to estimate the amount of stone removed before the exhausted tool was abandoned. An experimental test of Kuhn’s Index led Hiscock and Clarkson to suggest that it is an excellent estimator of flake mass loss. Our own tests, using different experimental procedures, reached much less optimistic conclusions. Here, we apply their own measurement procedures, and successfully duplicate their results. It does not follow, however, that our now-consistently high correlations have a causal connection. As GIUR values increase, they are less able to estimate the actual loss of flake mass. It appears, therefore, that incremental loss of flake mass need not be the sole driver of the GIUR value. If the correlation is an inevitable byproduct of the experiment’s design, as it now appears, then analyses based on this high correlation will be misleading. The GIUR remains, however, and excellent gauge of flake edge exhaustion.},
	journal = {Journal of Archaeological Science},
	author = {Eren, Metin I. and Sampson, C. Garth},
	year = {2009},
	keywords = {Experimental archaeology, Lithics, reduction index, Kuhn, Replication, Unifacial Retouch},
	pages = {1243--1247},
	file = {Eren and Sampson - 2009 - Kuhn’s Geometric Index of Unifacial Stone Tool Red.pdf:C\:\\Users\\guillermo_bustos_perez\\Zotero\\storage\\6PI58P8Q\\Eren and Sampson - 2009 - Kuhn’s Geometric Index of Unifacial Stone Tool Red.pdf:application/pdf},
}

@article{eren_defining_2005,
	title = {Defining and measuring reduction in unifacial stone tools},
	volume = {32},
	doi = {10.1016/j.jas.2005.03.003},
	abstract = {Observations pertaining to particular stages of the lithic chaıˆne ope´ratoire, or reconstructions of the entire operational sequence at a particular site, can be used to develop a detailed understanding of past human cognitive capabilities, technological sophistication, mobility, and land use. The ‘‘reduction sequence’’ is a specific stage of the chaîne opératoire that many archaeologists have attempted to measure. Many of these attempts fail to recognize that ‘‘reduction’’ is a three-dimensional process, and thus should be measured with an appropriate three-dimensional unit: volume. This paper presents a new methodology for measuring and defining reduction in unifacial stone tools that reconstructs the original volume of a modified blank, allowing a realistic percentage of volume loss to be calculated. This new method is fast, precise, and very accurate.},
	journal = {Journal of Archaeological Science},
	author = {Eren, Metin I. and Domínguez-Rodrigo, Manuel and Kuhn, Steven L. and Adler, David S. and Le, Ian and Bar-Yosef, Ofer},
	year = {2005},
	keywords = {Lithic technology, Experimental archaeology, Chaîne opératoire, Reduction sequence, Unifacial stone tools},
	pages = {1190--1201},
	file = {Eren et al. - 2005 - Defining and measuring reduction in unifacial ston.pdf:C\:\\Users\\guillermo_bustos_perez\\Zotero\\storage\\GCEWHRCW\\Eren et al. - 2005 - Defining and measuring reduction in unifacial ston.pdf:application/pdf},
}

