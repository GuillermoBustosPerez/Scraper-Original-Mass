	file = {Glauberman et al. - 2020 - Late Middle Paleolithic Technological Organization.pdf:C\:\\Users\\guillermo_bustos_perez\\Zotero\\storage\\HKVYLTK6\\Glauberman et al. - 2020 - Late Middle Paleolithic Technological Organization.pdf:application/pdf},
